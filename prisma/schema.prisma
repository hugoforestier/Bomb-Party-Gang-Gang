generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  u_id       BigInt  @id @default(autoincrement())
  u_password String? @db.VarChar
  u_username String  @unique @db.VarChar(30)
  u_salt     String? @db.VarChar
  u_uuid     String  @unique @db.Uuid
}
